{
  "name": "npm-manager",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile:js-vendor": "node scripts/compile-vendor.js",
    "compile:js-app": "node scripts/compile-app.js",
    "compile:js": "npm run compile:js-vendor & npm run compile:js-app",
    "compile:css": "node-sass public/src/sass/app.scss public/dist/style.css",
    "watch:js": "nodemon -e js -w public/src/js -x \"npm run compile:js-app\"",
    "watch:sass": "nodemon -e scss -w public/src/sass -x \"npm run compile:css\"",
    "compile": "npm run compile:js & npm run compile:css",
    "prestart": "npm run compile",
    "start": "nodemon app.js -e jade -w views"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ace-builds": "^1.2.2",
    "body-parser": "^1.14.2",
    "bootstrap-sass": "^3.3.6",
    "bootstrap-treeview": "^1.2.0",
    "concat": "^1.0.0",
    "directory-tree": "git+https://git@github.com/olivmonnier/node-directory-tree.git",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "jade": "^1.11.0",
    "jquery": "^2.2.0",
    "junk": "^1.0.2",
    "lodash": "^4.0.1",
    "node-sass": "^3.4.2",
    "nodemon": "^1.8.1",
    "ps-tree": "^1.0.1",
    "rimraf": "^2.5.1",
    "socket.io": "^1.4.5"
  }
}
